extends layout

block content
  h1= title
  
  ul
    - city_list.sort(function(a, b) {let textA = a.title.toUpperCase(); let textB = b.title.toUpperCase(); return (textA < textB) ? -1 : (textA > textB) ? 1 : 0;});
    each city in city_list
      li 
        a(href=city.url) #{city.title} 
        |  (#{city.startperiod}-#{city.endperiod)

    else
      li There are no cities that you can visit with us at the moment.